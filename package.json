{
  "name": "myesimplus-com",
  "version": "1.0.0",
  "description": "MyeSIMPlus website with Cloudflare integration",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --env production",
    "deploy:staging": "wrangler deploy --env staging",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "type-check": "tsc --noEmit",
    "build": "wrangler deploy --dry-run",
    "preview": "wrangler dev --env staging"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "hono": "^3.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "wrangler": "^4.45.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "vitest": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": ["esim", "cloudflare", "workers", "travel", "connectivity"],
  "author": "MyeSIMPlus Team",
  "license": "Apache-2.0"
}